{
  "root": "libs/solid/components",
  "sourceRoot": "libs/solid/components/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/solid/components/**/*.{ts,spec.ts}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/solid/components"],
      "options": {
        "jestConfig": "libs/solid/components/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
